        android:gravity="center"
        android:text="ADD Folder"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:backgroundTint="@color/Toolbar"
        app:cornerRadius="90sp">

    </com.google.android.material.button.MaterialButton>

</LinearLayout>
